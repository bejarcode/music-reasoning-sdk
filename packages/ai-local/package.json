{
  "name": "@music-reasoning/ai-local",
  "version": "0.1.0",
  "description": "Local AI runtime for Music Reasoning SDK - Offline music theory explanations powered by Phi-3-Mini-3.8B",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run tests/unit",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo"
  },
  "keywords": [
    "music-theory",
    "ai",
    "local-inference",
    "transformers"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@music-reasoning/core": "workspace:*",
    "@music-reasoning/types": "workspace:*",
    "node-llama-cpp": "^3.1.1"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^4.0.5"
  }
}
